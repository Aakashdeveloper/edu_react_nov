//es6
import React from 'react';
import ReactDOM from 'react-dom';
import Header from './component/header';
import Footer from './component/footer';

const App = () => {
    return(
        <React.Fragment>
            <Header/>
            <h1>My React App</h1>
            <h2>Edureka</h2>
            <Footer/>
        </React.Fragment>
    )
}

ReactDOM.render(<App/>,document.getElementById('root'))


